      "source": [
        "formula_1 = dfc[dfc['id_produto'] == 1]['durabilidade']\n",
        "formula_2 = dfc[dfc['id_produto'] == 2]['durabilidade']\n",
        "formula_3 = dfc[dfc['id_produto'] == 3]['durabilidade']\n",
        "\n",
        "# Realizando o teste de Kruskal-Wallis\n",
        "stat, p = kruskal(formula_1, formula_2, formula_3)\n",
        "\n",
        "print(f'Estatística H: {stat}')\n",
        "print(f'Valor p: {p}')\n",
        "\n",
        "# Interpretando o resultado\n",
        "alpha = 0.05\n",
        "if p < alpha:\n",
        "    print(\"Rejeitamos a hipótese nula: existe uma diferença significativa entre os grupos.\")\n",
        "else:\n",
        "    print(\"Não rejeitamos a hipótese nula: não há diferença significativa entre os grupos.\")"
      ],
      "metadata": {
