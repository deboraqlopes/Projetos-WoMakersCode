        "# Agrupar os dados por exposição ao sol\n",
        "grupo_expo_sol_0 = dfc[dfc['exposicao_sol'] == 0]['durabilidade']\n",
        "grupo_expo_sol_1 = dfc[dfc['exposicao_sol'] == 1]['durabilidade']\n",
        "\n",
        "# Realizando o teste de Kruskal-Wallis\n",
        "stat_sol, p_sol = kruskal(grupo_expo_sol_0, grupo_expo_sol_1)\n",
        "\n",
        "print(f'Estatística H (Exposição ao Sol): {stat_sol}')\n",
        "print(f'Valor p (Exposição ao Sol): {p_sol}')\n",
        "\n",
        "# Interpretando o resultado\n",
